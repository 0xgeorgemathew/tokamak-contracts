{
  "name": "@1inch/cross-chain-swap",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:1inch/cross-chain-swap.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@1inch/limit-order-protocol-contract": "4.0.0",
    "@1inch/solidity-utils": "4.1.0",
    "@matterlabs/hardhat-zksync-deploy": "1.3.0",
    "@matterlabs/hardhat-zksync-solc": "1.1.4",
    "@nomicfoundation/hardhat-chai-matchers": "2.0.6",
    "@nomicfoundation/hardhat-ethers": "3.0.5",
    "@nomicfoundation/hardhat-foundry": "1.1.1",
    "@nomicfoundation/hardhat-network-helpers": "1.0.10",
    "dotenv": "16.4.5",
    "eslint": "8.56.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "ethers": "6.11.1",
    "hardhat": "2.22.2",
    "hardhat-dependency-compiler": "1.1.4",
    "hardhat-gas-reporter": "1.0.9",
    "rimraf": "5.0.5",
    "solc": "0.8.23",
    "solhint": "4.1.1",
    "zksync-ethers": "6.7.0"
  },
  "scripts": {
    "clean": "rimraf coverage && forge clean",
    "coverage": "mkdir -p coverage && forge coverage --report lcov --ir-minimum --report-file coverage/lcov.info",
    "coverage:html": "bash scripts/coverage.sh",
    "doc": "forge doc --build --out documentation",
    "lint": "solhint --max-warnings 0 \"contracts/**/*.sol\" \"test/**/*.sol\"",
    "lint:js": "eslint .",
    "lint:fix": "solhint --max-warnings 0 \"contracts/**/*.sol\" \"test/**/*.sol\" --fix",
    "test": "FOUNDRY_PROFILE=default forge snapshot --no-match-test \"testFuzz_*\" && FOUNDRY_PROFILE=default forge test -vvv --gas-report",
    "test:lite": "FOUNDRY_PROFILE=lite forge test -vvv",
    "test:zksync": "FOUNDRY_PROFILE=zksync forge test -vvv --zksync"
  },
  "dependencies": {}
}
